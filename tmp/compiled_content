T;@%;@%o;;;;	I" /relax/2048undo/favicon.ico;
T{;{;o;;I"M/home/bing/Documents/blog/nanoc-plate/content/relax/2048undo/favicon.ico;
To;;;;	I" /relax/2048undo/2048auto.js;
