T;@V;@Vo;;;;	I"/relax/js/application.js;
T{;{;U;[I"J/home/bing/Documents/blog/nanoc-plate/content/relax/js/application.js;
TI"Å// Wait till the browser is ready to render the game (avoids glitches)
window.requestAnimationFrame(function () {
  new GameManager(4, KeyboardInputManager, HTMLActuator, LocalStorageManager);
});
