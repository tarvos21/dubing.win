T;@.;@.o;;;;	I"/relax/js/bind_polyfill.js;
T{;{;U;[I"L/home/bing/Documents/blog/nanoc-plate/content/relax/js/bind_polyfill.js;
TI"ÜFunction.prototype.bind = Function.prototype.bind || function (target) {
  var self = this;
  return function (args) {
    if (!(args instanceof Array)) {
      args = [args];
    }
    self.apply(target, args);
  };
};
