  -moz-font-feature-settings: "onum" 1, "pnum" 1, "afrc" 1, "kern" 1, "liga" 1, "dlig" 1;
  -ms-font-feature-settings: "onum" 1, "pnum" 1, "afrc" 1, "kern" 1, "liga" 1, "dlig" 1;
  -o-font-feature-settings: "onum" 1, "pnum" 1, "afrc" 1, "kern" 1, "liga" 1, "dlig" 1;
  -webkit-font-feature-settings: "onum" 1, "pnum" 1, "afrc" 1, "kern" 1, "liga" 1, "dlig" 1;
  font-feature-settings: "onum" 1, "pnum" 1, "afrc" 1, "kern" 1, "liga" 1, "dlig" 1; }

blockquote,
h1,
h2,
